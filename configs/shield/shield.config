CONFIG_WITH_LIBSHIELD=y
CONFIG_MAX_THREAD_PER_TASK=1

#
# Capabilities
#
# CONFIG_CAP_DEV_BUSES is not set
# CONFIG_CAP_DEV_IO is not set
# CONFIG_CAP_DEV_DMA is not set
# CONFIG_CAP_DEV_ANALOG is not set
# CONFIG_CAP_DEV_TIMER is not set
# CONFIG_CAP_DEV_STORAGE is not set
# CONFIG_CAP_DEV_CRYPTO is not set
# CONFIG_CAP_DEV_CLOCK is not set
# CONFIG_CAP_DEV_POWER is not set
# CONFIG_CAP_DEV_NEURAL is not set
# CONFIG_CAP_SYS_UPGRADE is not set
# CONFIG_CAP_SYS_POWER is not set
# CONFIG_CAP_SYS_PROCSTART is not set
# CONFIG_CAP_MEM_SHM_OWN is not set
# CONFIG_CAP_MEM_SHM_USE is not set
# CONFIG_CAP_MEM_SHM_TRANSFER is not set
# CONFIG_CAP_TIM_HP_CHRONO is not set
# CONFIG_CAP_CRY_KRNG is not set
# end of Capabilities

CONFIG_WITH_SENTRY=y

#
# Platform support
#
CONFIG_ARCH_ARM_CORTEX_M=y
CONFIG_ARCH_ARM=y
CONFIG_HAS_DCACHE=y
CONFIG_HAS_ICACHE=y
CONFIG_HAS_FPU=y
CONFIG_HAS_FPU_FPV5=y
CONFIG_HAS_MPU=y
CONFIG_HAS_MPU_PMSA_V8=y
CONFIG_NUM_MPU_REGIONS=8
CONFIG_HAS_NVIC=y
CONFIG_THUMB=y
CONFIG_LITTLE_ENDIAN=y
CONFIG_HAS_DWT=y
CONFIG_HAS_RNG=y
CONFIG_ARCH_ARM_ARMV8M=y
CONFIG_SOC_FAMILY_STM32=y
CONFIG_SOC_SUBFAMILY_STM32U5=y
CONFIG_SOC_SUBFAMILY_STM32U59_Ax=y
CONFIG_ARCH_ARM_CORTEX_NAME="m33"
CONFIG_ARCH_SOCNAME="STM32U5A5"
# CONFIG_ARCH_MCU_STM32F439 is not set
# CONFIG_ARCH_MCU_STM32F429 is not set
# CONFIG_ARCH_MCU_STM32F407 is not set
# CONFIG_ARCH_MCU_STM32F401 is not set
# CONFIG_ARCH_MCU_STM32WB55 is not set
# CONFIG_ARCH_MCU_STM32L476 is not set
CONFIG_ARCH_MCU_STM32U5A5=y
CONFIG_FPU_SOFT_ABI=y
# CONFIG_FPU_SOFTFP_ABI is not set
# CONFIG_FPU_HARDFP_ABI is not set
# CONFIG_USE_FPU is not set

#
# VFPv3 FPU not available for this target arch
#

#
# VFPv4 FPU not available for this target arch
#
CONFIG_FPU_ABI_CFLAGS="-mfloat-abi=soft"
CONFIG_ISA_CFLAGS="-mthumb"
CONFIG_ENDIANNESS_CFLAGS="-mlittle-endian"
CONFIG_SYSTICK_HZ=1000
# end of Platform support

#
# Core functions
#
CONFIG_MAX_TASKS=8
CONFIG_SCHED_RRMQ=y
# CONFIG_SCHED_FIFO is not set
# end of Core functions

#
# Managers
#

#
# Task manager
#
CONFIG_SVC_EXCHANGE_AREA_LEN=128
CONFIG_TASK_MAGIC_VALUE=0xdeadcafe
CONFIG_MAX_DEV_PER_TASK=4
CONFIG_MAX_SHM_PER_TASK=4
CONFIG_MAX_DMA_STREAMS_PER_TASK=4
# end of Task manager

#
# Debug manager
#
# CONFIG_DEBUG_COLORS is not set
CONFIG_DEBUG_LEVEL=7
# end of Debug manager

#
# Security manager
#
# CONFIG_SECU_ENFORCE_FAULT_INJECTION is not set
# end of Security manager
# end of Managers

#
# Build mode
#
# CONFIG_STANDALONE_MODE is not set
CONFIG_BUILD_WITH_GDB=y
CONFIG_BUILD_TARGET_DEBUG=y
# CONFIG_BUILD_TARGET_RELEASE is not set
# CONFIG_BUILD_TARGET_AUTOTEST is not set
# CONFIG_WITH_JTAG_CONNECTED is not set
# end of Build mode
